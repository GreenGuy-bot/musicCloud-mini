/* 水平间距 */
.navContainer {
  display: flex;
  width: 100%;
}
.navContainer .navItem {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.navContainer .navItem text {
  font-size: 26rpx;
}
.navContainer .navItem .iconfont {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  background-color: #f01313;
  font-size: 50rpx;
  color: #fff;
  margin: 20rpx 0;
}

